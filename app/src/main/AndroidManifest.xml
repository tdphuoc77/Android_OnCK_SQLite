<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onandroidck_sqllite">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnAndroidCK_SQLLITE">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerActivity">
        </activity>
        <activity android:name=".ListProductActivity">
        </activity>
        <activity android:name=".ProductAddActivity">
        </activity>
        <activity android:name=".ProductUpdateActivity">
        </activity>
        <activity android:name=".ProductDeleteActivity">
        </activity>
    </application>

</manifest>